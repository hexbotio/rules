{
  "rule": "Deploy Latest",
  "match": "@hex deploy latest*",
  "format": true,
  "acl": "matt",
  "help": "deploy latest <build #>",
  "actions": [
    { "type": "local", "command": "env", "config": { 
        "env": "PATH=/bin:/usr/bin",
        "dir": "/tmp"
      }, "hide_output": true
    },
    { "type": "local", "command": "echo ${hex.input.2} > ${hex.var.downloads}latest", "hide_output": true },
    { "type": "response", "command": "Setting latest to: ${hex.input.2}" }
  ]
}
